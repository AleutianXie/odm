{
	"name": "aimeos/ai-client-html",
	"description": "Aimeos ai-client-html extension",
	"keywords": ["aimeos", "extension", "client", "html"],
	"homepage": "https://aimeos.org/",
	"type": "aimeos-extension",
	"license": "LGPL-3.0-or-later",
	"support": {
		"source": "https://github.com/aimeos/ai-client-html",
		"issues": "https://github.com/aimeos/ai-client-html/issues",
		"forum": "https://aimeos.org/help",
		"wiki": "https://aimeos.org/docs"
	},
	"prefer-stable": true,
	"minimum-stability": "dev",
	"require": {
		"php": ">=5.5.0",
		"composer/installers": "^1.0",
		"aimeos/aimeos-core": "~2019.04",
		"aimeos/ai-controller-jobs": "~2019.04",
		"aimeos/ai-controller-frontend": "~2019.04"
	},
	"autoload": {
		"psr-4": {
			"Aimeos\\Client\\Html\\": "client/html/src/Client/Html/",
			"Aimeos\\Controller\\Jobs\\": "controller/jobs/src/Controller/Jobs/",
			"Aimeos\\Controller\\Common\\": "controller/common/src/Controller/Common/"
		},
		"classmap": [
			"client/html/src",
			"controller/jobs/src",
			"controller/common/src"
		]
	}
}